function setgopath() {
	if [ -d "$1" ]; then
		export GOPATH=$1
		echo "GOPATH set to: $GOPATH"
	else
		echo "Can't set GOPATH because directory doesn't exist"
	fi	
}

function gop() {
	cd $GOPATH
}

function gocd () { 
	cd `go list -f '{{.Dir}}' $1` 
}

# Set Triton's environment
function tenv() {
	eval "$(triton env --docker ${1})"
	docker info
}

